<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Plain.
    -->
    <changeSet id="20190914033722-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="the_enaction_description_id"
                                 baseTableName="plain"
                                 constraintName="fk_plain_the_enaction_description_id"
                                 referencedColumnNames="id"
                                 referencedTableName="enaction_description"/>

        <addForeignKeyConstraint baseColumnNames="the_automatic_sub_id"
                                 baseTableName="plain"
                                 constraintName="fk_plain_the_automatic_sub_id"
                                 referencedColumnNames="id"
                                 referencedTableName="automatic"/>

        <addForeignKeyConstraint baseColumnNames="the_normal_sub_id"
                                 baseTableName="plain"
                                 constraintName="fk_plain_the_normal_sub_id"
                                 referencedColumnNames="id"
                                 referencedTableName="normal"/>

    </changeSet>
</databaseChangeLog>
