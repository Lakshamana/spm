<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity RequiredPeople.
    -->
    <changeSet id="20190914033829-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="the_req_work_group_sub_id"
                                 baseTableName="required_people"
                                 constraintName="fk_required_people_the_req_work_group_sub_id"
                                 referencedColumnNames="id"
                                 referencedTableName="req_work_group"/>

        <addForeignKeyConstraint baseColumnNames="the_req_agent_sub_id"
                                 baseTableName="required_people"
                                 constraintName="fk_required_people_the_req_agent_sub_id"
                                 referencedColumnNames="id"
                                 referencedTableName="req_agent"/>

        <addForeignKeyConstraint baseColumnNames="the_normal_id"
                                 baseTableName="required_people"
                                 constraintName="fk_required_people_the_normal_id"
                                 referencedColumnNames="id"
                                 referencedTableName="normal"/>

    </changeSet>
</databaseChangeLog>
