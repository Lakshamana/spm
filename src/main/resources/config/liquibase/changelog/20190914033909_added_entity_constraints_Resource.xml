<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Resource.
    -->
    <changeSet id="20190914033909-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="the_consumable_sub_id"
                                 baseTableName="resource"
                                 constraintName="fk_resource_the_consumable_sub_id"
                                 referencedColumnNames="id"
                                 referencedTableName="consumable"/>

        <addForeignKeyConstraint baseColumnNames="the_exclusive_sub_id"
                                 baseTableName="resource"
                                 constraintName="fk_resource_the_exclusive_sub_id"
                                 referencedColumnNames="id"
                                 referencedTableName="exclusive"/>

        <addForeignKeyConstraint baseColumnNames="the_shareable_sub_id"
                                 baseTableName="resource"
                                 constraintName="fk_resource_the_shareable_sub_id"
                                 referencedColumnNames="id"
                                 referencedTableName="shareable"/>

        <addForeignKeyConstraint baseColumnNames="belongs_to_id"
                                 baseTableName="resource"
                                 constraintName="fk_resource_belongs_to_id"
                                 referencedColumnNames="id"
                                 referencedTableName="resource"/>

        <addForeignKeyConstraint baseColumnNames="the_resource_type_id"
                                 baseTableName="resource"
                                 constraintName="fk_resource_the_resource_type_id"
                                 referencedColumnNames="id"
                                 referencedTableName="resource_type"/>

    </changeSet>
</databaseChangeLog>
