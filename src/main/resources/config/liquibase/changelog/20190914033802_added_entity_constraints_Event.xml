<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Event.
    -->
    <changeSet id="20190914033802-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="the_catalog_events_id"
                                 baseTableName="event"
                                 constraintName="fk_event_the_catalog_events_id"
                                 referencedColumnNames="id"
                                 referencedTableName="catalog_event"/>

        <addForeignKeyConstraint baseColumnNames="the_task_id"
                                 baseTableName="event"
                                 constraintName="fk_event_the_task_id"
                                 referencedColumnNames="id"
                                 referencedTableName="task"/>

        <addForeignKeyConstraint baseColumnNames="the_log_id"
                                 baseTableName="event"
                                 constraintName="fk_event_the_log_id"
                                 referencedColumnNames="id"
                                 referencedTableName="spm_log"/>

        <addForeignKeyConstraint baseColumnNames="the_event_type_id"
                                 baseTableName="event"
                                 constraintName="fk_event_the_event_type_id"
                                 referencedColumnNames="id"
                                 referencedTableName="event_type"/>

        <addForeignKeyConstraint baseColumnNames="the_activity_id"
                                 baseTableName="event"
                                 constraintName="fk_event_the_activity_id"
                                 referencedColumnNames="id"
                                 referencedTableName="activity"/>

        <addForeignKeyConstraint baseColumnNames="the_plain_id"
                                 baseTableName="event"
                                 constraintName="fk_event_the_plain_id"
                                 referencedColumnNames="id"
                                 referencedTableName="plain"/>

        <addForeignKeyConstraint baseColumnNames="the_agent_id"
                                 baseTableName="event"
                                 constraintName="fk_event_the_agent_id"
                                 referencedColumnNames="id"
                                 referencedTableName="agent"/>

    </changeSet>
</databaseChangeLog>
