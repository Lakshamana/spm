<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Subroutine.
    -->
    <changeSet id="20190914033919-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="the_class_method_call_sub_id"
                                 baseTableName="subroutine"
                                 constraintName="fk_subroutine_the_class_method_call_sub_id"
                                 referencedColumnNames="id"
                                 referencedTableName="class_method_call"/>

        <addForeignKeyConstraint baseColumnNames="the_script_sub_id"
                                 baseTableName="subroutine"
                                 constraintName="fk_subroutine_the_script_sub_id"
                                 referencedColumnNames="id"
                                 referencedTableName="script"/>

        <addForeignKeyConstraint baseColumnNames="the_artifact_type_id"
                                 baseTableName="subroutine"
                                 constraintName="fk_subroutine_the_artifact_type_id"
                                 referencedColumnNames="id"
                                 referencedTableName="artifact_type"/>

    </changeSet>
</databaseChangeLog>
