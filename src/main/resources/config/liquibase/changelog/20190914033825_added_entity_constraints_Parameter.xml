<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Parameter.
    -->
    <changeSet id="20190914033825-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="the_artifact_parameter_sub_id"
                                 baseTableName="parameter"
                                 constraintName="fk_parameter_the_artifact_parameter_sub_id"
                                 referencedColumnNames="id"
                                 referencedTableName="artifact_param"/>

        <addForeignKeyConstraint baseColumnNames="the_primitive_param_sub_id"
                                 baseTableName="parameter"
                                 constraintName="fk_parameter_the_primitive_param_sub_id"
                                 referencedColumnNames="id"
                                 referencedTableName="primitive_param"/>

        <addForeignKeyConstraint baseColumnNames="the_automatic_id"
                                 baseTableName="parameter"
                                 constraintName="fk_parameter_the_automatic_id"
                                 referencedColumnNames="id"
                                 referencedTableName="automatic_activity"/>

    </changeSet>
</databaseChangeLog>
